{
  "indent_style": "spaces",
  "indent_size": 2,
  "max_line_length": 100,
  "brace_style": "1tbs",
  "space_before_function_paren": true,
  "newline_before_return": true,
  "newline_per_chained_call": true,
  "require_semicolons": true,
  "space_after_keywords": true,
  "space_after_control_statements": true,
  "space_before_block_statements": true,
  "camel_case_variables": true,
  "camel_case_functions": true,
  "always_use_braces": true,
  "newline_after_open_brace": true,
  "newline_before_close_brace": true,
  "newline_after_comma": true,
  "space_after_comma": true,
  "space_before_comma": false,
  "space_after_colon": true,
  "space_before_colon": false,
  "newline_after_colon": true,
  "space_around_operators": true,
  "remove_trailing_whitespace": true,
  "remove_trailing_commas": true,
  "trim_whitespace": true
}

